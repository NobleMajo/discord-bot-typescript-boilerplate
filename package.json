{
  "name": "discord-bot-typescript-boilerplate",
  "version": "1.0.0",
  "description": "![MIT](https://img.shields.io/badge/license-MIT-blue.svg) ![typescript](https://img.shields.io/badge/dynamic/json?style=plastic&color=blue&label=Typescript&prefix=v&query=devDependencies.typescript&url=https%3A%2F%2Fraw.githubusercontent.com%2Fnoblemajo%2Fdiscord-bot-typescript-boilerplate%2Fmain%2Fpackage.json) ![github](https://img.shields.io/badge/dynamic/json?style=plastic&color=darkviolet&label=GitHub&prefix=v&query=version&url=https%3A%2F%2Fraw.githubusercontent.com%2Fnoblemajo%2Fdiscord-bot-typescript-boilerplate%2Fmain%2Fpackage.json)",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "start": "ts-node ./src/index.ts",
    "exec": "node dist/index.js",
    "test": "mocha --paralell --exit --require \"ts-node/register\" \"./src/test/**/*.test.ts\"",
    "build": "npm run tsc",
    "start:watch": "nodemon -w ./src -x \"npm run start\" --ext \"*.ts\"",
    "build:watch": "nodemon -w ./src -x \"npm run build\" --ext \"*.ts\"",
    "test:watch": "nodemon -w ./src -x \"npm run test\" --ext \"*.ts\"",
    "exec:watch": "nodemon -w ./dist -x \"npm run exec\"",
    "dev": "nodemon -w ./src -x \"npm run start\" --ext \"*.ts\""
  },
  "author": "NobleMajo",
  "license": "MIT",
  "devDependencies": {
    "@types/json5": "^2.2.0",
    "@types/node": "^20.3.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "discord.js": "^14.11.0",
    "json5": "^2.2.3",
    "typenvy": "^1.5.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NobleMajo/discord-bot-typescript-boilerplate.git"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/NobleMajo/discord-bot-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/NobleMajo/discord-bot-typescript-boilerplate#readme"
}
